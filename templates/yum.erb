# This file is managed by puppet
# module 'packagecloud'

[<%= @normalized_name %>]
name=<%= @description %>
baseurl=<%= @repo_url %>
repo_gpgcheck=<%= @repo_gpgcheck %>
<% if @priority -%>
priority=<%=@priority %>
<% end -%>
gpgcheck=0
enabled=1
gpgkey=file://<%= @gpg_file_path %>
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
