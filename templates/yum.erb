# This file is managed by puppet
# module 'packagecloud'

[<%= @normalized_name %>]
name=<%= @description %>
baseurl=<%= @repo_url %>
<% if @priority -%>
priority=<%=@priority %>
<% end -%>
gpgcheck=1
<% if @enabled -%>
enabled=1
<% else %>
enabled=0
<% end -%>
gpgkey=file://<%= @gpg_file_path %>
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
<% if @proxy -%>
proxy=<%=@proxy %>
<% end -%>